  "name": "complements",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "gulp && tsc && vite build",
    "build:server": "tsc && vite build --ssr",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:watch": "cross-env NODE_ENV=production nodemon server.js",
    "lint": "eslint src --ext ts,tsx,js --max-warnings 0 --fix",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:ui": "vitest --run --ui",
    "test:watch": "vitest",
    "test:watch:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "stylelint:fix": "stylelint 'src/**/*{.css,.scss}' --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "nanoid": "^4.0.2",
    "normalize.css": "^8.0.1",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "sort-by": "^1.2.0",
    "vite-plugin-import-build": "^0.2.3",
    "vite-plugin-ssr": "^0.4.136"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.34.3",
    "autoprefixer": "^10.4.14",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-webp": "^4.0.1",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "msw": "^1.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "sass": "^1.64.0",
    "sharp": "^0.32.5",
    "stylelint": "^15.10.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.1.0",
    "svgo": "^3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vitest": "^0.34.3"
  }
}
